<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<NamingStandardsTemplate xmlns="http://www.w3.org/2001/XMLSchema-instance" NSTemplateDesc="Sample naming standard template." NSTemplateName="Sample Naming Standard">

  <LogicalObjectConventions>
    <ObjectConvention CaseConvention="[none]" ConstraintConvention="" MaxLength="1024" ObjectType="Entity" Prefix="" Suffix=""/>
    <ObjectConvention CaseConvention="[none]" ConstraintConvention="" MaxLength="1024" ObjectType="Attribute" Prefix="" Suffix=""/>
    <ObjectConvention CaseConvention="[none]" ConstraintConvention="" MaxLength="1024" ObjectType="View" Prefix="" Suffix=""/>
    <ObjectConvention CaseConvention="[none]" ConstraintConvention="" MaxLength="1024" ObjectType="Key" Prefix="" Suffix=""/>
    <ObjectConvention CaseConvention="[none]" ConstraintConvention="" MaxLength="1024" ObjectType="Relationship" Prefix="" Suffix=""/>
  </LogicalObjectConventions>

  <PhysicalObjectConventions>
    <ObjectConvention CaseConvention="[none]" ConstraintConvention="" MaxLength="30" ObjectType="Table" Prefix="" Suffix=""/>
    <ObjectConvention CaseConvention="[none]" ConstraintConvention="" MaxLength="30" ObjectType="Column" Prefix="" Suffix=""/>
    <ObjectConvention CaseConvention="[none]" ConstraintConvention="" MaxLength="30" ObjectType="View" Prefix="" Suffix=""/>
    <ObjectConvention CaseConvention="[none]" ConstraintConvention="" MaxLength="30" ObjectType="Index" Prefix="" Suffix=""/>
    <ObjectConvention CaseConvention="[none]" ConstraintConvention="" MaxLength="30" ObjectType="Foreign Key" Prefix="" Suffix=""/>
  </PhysicalObjectConventions>

  <Abbreviations>
    <Abbreviation IsClass="False" IsIllegal="False" IsPrime="False" IsQualifier="False" LogicalText="OOne" PhysicalText="Won" Priority="Primary"/>
    <Abbreviation IsClass="False" IsIllegal="False" IsPrime="False" IsQualifier="False" LogicalText="One" PhysicalText="1" Priority="Primary"/>
    <Abbreviation IsClass="False" IsIllegal="False" IsPrime="False" IsQualifier="False" LogicalText="Room" PhysicalText="Rm" Priority="Primary"/>
