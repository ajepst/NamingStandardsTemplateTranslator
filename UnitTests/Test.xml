    <Abbreviation IsClass="False" IsIllegal="False" IsPrime="False" IsQualifier="False" LogicalText="Average" PhysicalText="Ave" Priority="Primary"/>
    <Abbreviation IsClass="False" IsIllegal="False" IsPrime="False" IsQualifier="False" LogicalText="Count" PhysicalText="Cnt" Priority="Primary"/>
    <Abbreviation IsClass="False" IsIllegal="False" IsPrime="False" IsQualifier="False" LogicalText="Eight" PhysicalText="8" Priority="Primary"/>
