  <PhysicalOrder>
    <Order ObjectType="Table" Part1="..." Part2="..." Part3="..."/>
    <Order ObjectType="Column" Part1="..." Part2="..." Part3="..."/>
    <Order ObjectType="View" Part1="..." Part2="..." Part3="..."/>
    <Order ObjectType="Index" Part1="..." Part2="..." Part3="..."/>
    <Order ObjectType="Foreign Key" Part1="..." Part2="..." Part3="..."/>
